  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "cont",
              "section": "Content Changes"
            },
            {
              "type": "other",
              "section": "Other Minor Changes"
            },
            {}
          ]
        }
      }
    }
  },
  "devDependencies": {
    "@capacitor/cli": "^4.4.0",
    "@types/howler": "^2.2.7",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^4.9.5",
    "webpack-cli": "^4.10.0"
  }
}
